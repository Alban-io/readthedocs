{
	"index_patterns": [
		"*-log"
	],
	"settings": {
		"number_of_shards": 16,
		"number_of_replicas": 1
	},
	"mappings": {
		"log": {
			"dynamic": "false",
			"properties": {
				"address": {
					"type": "text",
					"fields": {
						"raw": {
							"type": "keyword",
							"ignore_above": 256
						}
					}
				},
				"blockHash": {
					"type": "keyword",
					"ignore_above": 256
				},
				"id": {
					"type": "keyword",
					"ignore_above": 256
				},
				"blockNumber": {
					"properties": {
						"num": {
							"type": "long"
						},
						"raw": {
							"type": "keyword",
							"ignore_above": 256
						}
					}
				},
				"data": {
					"type": "keyword",
					"ignore_above": 256
				},
				"logIndex": {
					"properties": {
						"num": {
							"type": "long"
						},
						"raw": {
							"type": "keyword",
							"ignore_above": 256
						}
					}
				},
				"removed": {
					"type": "boolean"
				},
				"signature": {
					"type": "keyword",
					"ignore_above": 256
				},
				"timestamp": {
					"type": "date"
				},
				"topics": {
					"type": "keyword",
					"ignore_above": 256
				},
				"transactionHash": {
					"type": "keyword",
					"ignore_above": 256
				},
				"transactionIndex": {
					"properties": {
						"num": {
							"type": "long"
						},
						"raw": {
							"type": "keyword",
							"ignore_above": 256
						}
					}
				}
			}
		}
	}
}